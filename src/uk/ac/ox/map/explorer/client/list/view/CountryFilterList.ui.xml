<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:uk.ac.ox.map.explorer.client.filter.view">
	
  <ui:with type="uk.ac.ox.map.explorer.client.resource.ResourceBundle" field="res"></ui:with>
	<g:HTMLPanel>
	 <g:VerticalPanel>
     <g:Label styleName="{res.panelCss.panelHeader}">Filter countries list</g:Label>
	   <f:BooleanFilter propertyName="pfEndemic" ui:field="pfEndemic" labelText="Pf endemic" />
	   <f:BooleanFilter propertyName="pvEndemic" ui:field="pvEndemic" labelText="Pv endemic" />
	   <f:OptionFilter labelText="Continent" propertyName="continent" jsVar="Continent" ui:field="continent"/>
	 </g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 