<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:c="urn:import:com.google.gwt.user.cellview.client">
  <ui:style>
    .infoPanel {
    	background-color: #cccccc;
    }
  </ui:style>
  
  <ui:with type="uk.ac.ox.map.explorer.client.resource.ResourceBundle" field="res"></ui:with>
  <g:HTMLPanel>
    <g:Label text="Display summary" styleName="{res.panelCss.panelHeader}"/>
    <table>
      <tr>
        <td>
          <g:Label ui:field="siteCount" />
        </td>
        <td>sites</td>
      </tr>
      <tr>
        <td>
          <g:Label ui:field="surveyReplicateCount" />
        </td>
        <td>unique site-date combinations</td>
      </tr>
    </table>
    <!-- <c:CellList ui:field=""> </c:CellList> -->
  </g:HTMLPanel>
</ui:UiBinder> 