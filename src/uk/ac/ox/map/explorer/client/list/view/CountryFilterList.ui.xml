<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:f="urn:import:uk.ac.ox.map.explorer.client.filter.view">
	
  <ui:with type="uk.ac.ox.map.explorer.client.resource.ResourceBundle" field="res"></ui:with>
	<g:HTMLPanel>
	 <g:VerticalPanel>
     <g:Label styleName="{res.panelCss.panelHeader}">Filter countries</g:Label>
	   <f:BooleanFilter labelText="Pf endemic" propertyName="pfEndemic" ui:field="pfEndemic"/>
	   <f:BooleanFilter labelText="Pv endemic" propertyName="pvEndemic" ui:field="pvEndemic"/>
	   <f:OptionFilter labelText="Continent" propertyName="continent" jsVar="Continent" ui:field="continent"/>
	 </g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 