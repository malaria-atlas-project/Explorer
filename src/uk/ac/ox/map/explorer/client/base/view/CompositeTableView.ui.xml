<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
    .tablePanel {
    	background-color: #ebebeb;
    }
  </ui:style>
  <!-- Data browser -->
  <g:LayoutPanel>
    <g:layer top="0px" height="500px">
      <g:SimplePanel ui:field="tablePanel" styleName="{style.tablePanel}" />
    </g:layer>
    <g:layer top="500px" bottom="0px">
      <g:SimplePanel styleName="" />
    </g:layer>
  </g:LayoutPanel>
</ui:UiBinder> 